#include <stdio.h>
#include <string.h>

#define MAX_PATH 256
#define MAX_LINE 1024

int vybor(int *n) {
    return scanf("%d", n) == 1;
}

int file(const char *path) {
    FILE *f = fopen(path, "r");
    if (f == NULL) return 0;

    int pust = 1;
    char ch;
    while ((ch = fgetc(f)) != EOF) {
        putchar(ch);
        pust = 0;
    }
    fclose(f);
    return !pustf;
}

int load_file(char *path) {
    if (scanf("%255s", path) != 1) return 0;
    return file(path);
}

int write(const char *path) {
    char line[MAX_LINE];
    scanf(" ");
    if (!fgets(line, MAX_LINE, stdin)) return 0;

    size_t len = strlen(line);
    if (len > 0 && line[len - 1] == '\n') {
        line[len - 1] = '\0';
    }

    FILE *f = fopen(path, "a");
    if (f == NULL) return 0;

    fprintf(f, "\n%s", line);
    fclose(f);
    return file(path);
}

int main() {
    int n = 0;
    int loaded = 0;
    char path[MAX_PATH] = "";
    int running = 1;

    while (running) {
        if (!vybor(&n)) {
            printf("n/a\n");
            return 0;
        }

        if (n == -1) return 0;

        if (n == 1) {
            if (load_file(path)) {
                loaded = 1;
            } else {
                printf("n/a\n");
                loaded = 0;
            }
            printf("\n");
        }

        if (n == 2) {
            if (!loaded || !write(path)) {
                printf("n/a\n");
            }
            printf("\n");
        }
    }

    return 0;
}
