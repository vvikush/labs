#include <stdio.h>
#define NMAX 10

int input(int *a, int *n);
void output(int *a, int n);
int max(int *a, int n);
int min(int *a, int n);
double mean(int *a, int n);
double variance(int *a, int n);

void output_result(int max_v, int min_v, double mean_v, double variance_v);

int main() {
    int n, data[NMAX];

    if (!input(data, &n) || n <= 0 || n > NMAX) {
        printf("n/a\n");
        return 1;
    }
        output(data, n);
    
        output_result(max(data, n), min(data, n), mean(data, n), variance(data, n));

        return 0;
    }

    int input(int *a, int *n) {
        if (scanf("%d", n) != 1) return 0;
        for (int i = 0; i < *n; i++) {
            if (scanf("%d", &a[i]) != 1) return 0;
        }
        char c;
        if ((c = getchar()) != '\n' && c != EOF) {
            return 0;
        }
        return 1;
    }

    void output(int *a, int n) {
        for (int i = 0; i < n; i++) {
            printf("%d", a[i]);
            if (i < n - 1) printf(" ");
        }
        printf("\n");
    }

    int max(int *a, int n) {
        int max_v = a[0];
        for (int i = 1; i < n; i++) {
            if (a[i] > max_v) max_v = a[i];
        }
        return max_v;
    }

    int min(int *a, int n) {
        int min_v = a[0];
        for (int i = 1; i < n; i++) {
            if (a[i] < min_v) min_v = a[i];
        }
        return min_v;
    }

    double mean(int *a, int n) {
        double sum = 0;
        for (int i = 1; i < n; i++) {
            sum = sum + a[i];
        }
        return sum / n;
    }

    double variance(int *a, int n) {
        double m = mean(a, n);
        double sum = 0;
        for (int i = 1; i < n; i++) {
            double d = a[i] - m;
            sum = sum + d * d;
        }
        return sum / n;
    }

    void output_result(int max_v, int min_v, double mean_v, double variance_v);
    printf("%d %d %.6lf %.6lf\n", max_v, min_v, mean_v, variance_v);
