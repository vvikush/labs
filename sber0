#include <stdio.h>

#define LEN 100

void sum(int *buff1, int len1, int *buff2, int len2, int *result, int *result_length);
void sub(int *buff1, int len1, int *buff2, int len2, int *result, int *result_length);

int input(int *a, int *len);
void output(int *a, int len);
int is_greater_or_equal(int *a, int len1, int *b, int len2);

int main() {
    int buff1[LEN], buff2[LEN];
    int res_sum[LEN + 1], len_sum;
    int res_sub[LEN], len_sub;
    int len1, len2;

    if (input(buff1, &len1) && input(buff2, &len2)) {
        sum(buff1, len1, buff2, len2, res_sum, &len_sum);
        output(res_sum, len_sum);

        if (is_greater_or_equal(buff1, len1, buff2, len2)) {
            sub(buff1, len1, buff2, len2, res_sub, &len_sub);
            output(res_sub, len_sub);
        } else {
            printf("n/a\n");
        }
    } else {
        printf("n/a\n");
    }

    return 0;
}

int input(int *a, int *len) {
    int val, ok = 1;
    char c;
    *len = 0;

    while (scanf("%d", &val) == 1) {
        if (val < 0 || val > 9 || *len >= LEN) {
            ok = 0;
        }
        a[(*len)++] = val;*]()
